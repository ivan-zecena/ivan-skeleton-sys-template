	<apikit:config name="version-template-config" api="resource::8db5810f-1022-47cc-856e-2156f651647f:version-template:1.0.0:raml:zip:version-template.raml" outboundHeadersMapName="outboundHeaders" httpStatusVarName="httpStatus" />
	<api-gateway:autodiscovery apiId="${api.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="aa11e981-88fa-4611-a453-f45fb2990724" flowRef="main-flow"/>
	<configuration-properties doc:name="Configuration properties" doc:id="32c1d7b6-d093-4b09-b3f5-636904cbe3c5" file="properties\git.properties" />
	<configuration-properties doc:name="Configuration properties" doc:id="68dd8d82-2131-4576-875c-a7975e92e60d" file="properties\configurations\config-common.yaml" />
